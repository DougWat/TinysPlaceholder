{
  "name": "TinysPlaceholder",
  "version": "1.0.0",
  "scripts": {
    "build": "SET NODE_ENV=production npm-run-all build:*",
    "dev": "npm-run-all --parallel dev:*",
    "github-build": "npm-run-all build:*",
    "clean": "rm -rf _site",
    "build:postcss": "postcss src/styles/styles.css -o dist/assets/styles/styles.css",
    "build:eleventy": "eleventy --pathprefix 'TinysPlaceholder'",
    "dev:postcss": "postcss src/styles/styles.css -o dist/assets/styles/styles.css -w",
    "dev:eleventy": "npx @11ty/eleventy --serve"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.7",
    "postcss": "^8.5.4",
    "postcss-cli": "^11.0.1",
    "postcss-custom-media": "^11.0.6",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^7.0.2",
    "postcss-simple-vars": "^7.0.1"
  }
}
